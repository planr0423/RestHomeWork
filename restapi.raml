#%RAML 1.0
title: ClassifyWithTree
version: v1
baseUri: http://api.samplehost.com
protocols: HTTP
description:
  - Micro-Service to Classify data with tree
mediaType: application/xml
types:
  existData:
    type: object
    properties:
      color: string
      name: string
  DataList:
    type: object
    properties:
      Name: string
      Path: string


/api/Data:
  description: Data API root route
  post:
    description: post Data to service
    displayName: postData
    body:
      application/json:
        type: existData
    responses:
      200:
        body:
          application/json:
            type: DataList
    /{DataName}:
    get:
      description: Search Data by name
      displayName: findDataByName
      queryParameters:
        name: string
      responses:
        200:
          body:
            application/xml
              type: Datalist
        403:
        404:
  /TotalList:
    get:
      description: Get Total Data Xml information
      displayName: TotalXml
      responses:
        200:
          body:
            application/xml:
              type: DataList
        403ï¼š
        404